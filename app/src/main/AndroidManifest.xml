<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.dual_diagnosis">


    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        tools:replace="android:icon"
        android:usesCleartextTraffic="true"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity
            android:name=".Dual_Diagnosis"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".Questionaries"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".AnxietyForm"
            android:theme="@style/AppTheme"/>
        <activity
            android:name=".SplashActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".DetailsActivity"
            android:theme="@style/AppTheme"/>


        <activity android:name=".Main_Activity_Module"
            android:theme="@style/AppTheme"/>

        <activity android:name=".ForgotPassword"
            android:theme="@style/AppTheme"/>

        <activity android:name=".About_Us_Activity"
            android:theme="@style/AppTheme"/>

        <activity
            android:name=".Login_Activity"
            android:label="@string/title_activity_login_"
            android:theme="@style/AppTheme"></activity>


        <activity android:name=".SignUpActivity"
            android:theme="@style/AppTheme"/>

    </application>

</manifest>